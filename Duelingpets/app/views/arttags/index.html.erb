<% provide(:title, "Duelingpets: Arttags") %>
<% provide(:description, "All of the tags that are applied to the users artwork.") %>
<% provide(:keywords, "virtualpet, digitalpets, artwork") %>
<% provide(:robots, "noindex, nofollow") %>
<% provide(:h1, "Arttag List") %>
<div class="formlinks"><%= paginate @arttags %></div>
<br>
