/* Activepet sidebar */
.activepet{text-align: right; color:<%= getColorAttribute("Profile") %>; padding: 0% 8%}
.activepet a{color:<%= getColorAttribute("Profile") %>}
.activepet a:visited{<%= getColorAttribute("Profilevisited") %>}
.activepet a:hover{color: <%= getColorAttribute("Profilehovercolor") %>; background: <%= getColorAttribute("Profilehoverbackgc") %>}
/*Background needs its own color in the created menu*/
.sidenav{width: 130px;position: absolute;z-index: 1;top: 455px;right: 25px; /*background:*/ overflow-x: hidden;padding: 8px 0px;}
#maincontent{margin-right: 155px; padding: 0px 10px;}
/* End of Activepet */

/* Handles the page content features */
#subheader{font-weight: bold;text-align: center;font-size: 120%; color: <%= getColorAttribute("Header") %>; font-family: 'Chalkduster', 'Apple Chancery', 'Comic Sans MS', 'fantasy'}
.button{color: <%= getColorAttribute("Defaultbuttoncolor") %>; background: <%= getColorAttribute("Defaultbuttonbackgc") %>}
.editbutton{color: <%= getColorAttribute("Editbuttoncolor") %>; background: <%= getColorAttribute("Editbuttonbackgc") %>}
.destroybutton{color: <%= getColorAttribute("Destroybuttoncolor") %>; background: <%= getColorAttribute("Destroybuttonbackgc") %>}
.submitbutton{color: <%= getColorAttribute("Submitbuttoncolor") %>; background: <%= getColorAttribute("Submitbuttonbackgc") %>}
.explantext{color: <%= getColorAttribute("Explantextcolor") %>;}
.formlinks {text-align: center;}
