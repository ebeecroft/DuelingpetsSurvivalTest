.subheader2{font-weight: bold;text-align: center;font-size: 160%; color: <%= getColorAttribute("Subheader2") %>; font-family: 'Chalkduster', 'Apple Chancery', 'Comic Sans MS', 'fantasy'}
.subheader3{font-weight: bold;text-align: center;font-size: 160%; color: <%= getColorAttribute("Subheader3") %>; font-family: 'Chalkduster', 'Apple Chancery', 'Comic Sans MS', 'fantasy'}
.hublinks a{color: <%= getColorAttribute("Profile") %>;}
/* Mainpage content end */

/* Handles the indentation of the alert features */
.alert-success{color: <%= getColorAttribute("Success") %>}
.alert-error{color: <%= getColorAttribute("Error") %>}
.alert-firstnameerror{color: <%= getColorAttribute("Error") %>}
.alert-lastnameerror{color: <%= getColorAttribute("Error") %>}
.alert-loginerror{color: <%= getColorAttribute("Error") %>}
.alert-vnameerror{color: <%= getColorAttribute("Error") %>}
#errorheader{text-align: left;font-weight: bold;padding: 5px 5px 5px 15px;font-size: 12px; margin: -7px;margin-bottom: 0px;background-color: <%= getColorAttribute("Explanheaderbackgc") %>;color: <%= getColorAttribute("Explanheadercolor") %>;} ul li {font-size: 12px;list-style: square;}
#errorexplan {width: 450px;border: 2px solid <%= getColorAttribute("Explanationborder") %>;padding: 7px;padding-bottom: 0;margin-bottom: 20px;background-color: <%= getColorAttribute("Explanationbackgc") %>;}
.field_with_errors{padding: 2px;background-color: <%= getColorAttribute("Errorfieldcolor") %>;display: inherit; text-align: center;}
