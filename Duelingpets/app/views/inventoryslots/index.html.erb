<% provide(:title, "Duelingpets: Inventoryslots") %>
<% provide(:description, "Displays the item stats in each inventoryslot.") %>
<% provide(:keywords, "virtualpet, digitalpets, inventoryslot, item") %>
<% provide(:robots, "noindex, nofollow") %>
<% provide(:h1, "Inventoryslot List") %>
<div class="formlinks"><%= paginate @inventoryslots %></div>
<br>
