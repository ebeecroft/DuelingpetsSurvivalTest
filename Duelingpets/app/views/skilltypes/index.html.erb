<p id="notice"><%= notice %></p>

<h1>Skilltypes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skilltypes.each do |skilltype| %>
      <tr>
        <td><%= skilltype.name %></td>
        <td><%= link_to 'Show', skilltype %></td>
        <td><%= link_to 'Edit', edit_skilltype_path(skilltype) %></td>
        <td><%= link_to 'Destroy', skilltype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skilltype', new_skilltype_path %>
