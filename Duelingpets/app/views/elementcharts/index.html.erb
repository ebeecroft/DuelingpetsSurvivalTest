<% provide(:title, "Duelingpets: Elementcharts") %>
<% provide(:description, "Charts for the various battle elements.") %>
<% provide(:keywords, "virtualpet, digitalpets, battle, elements") %>
<% provide(:robots, "noindex, nofollow") %>
<% provide(:h1, "Elementchart List") %>
<% if(@elementcharts.count >= 10) %>
   <div class="formlinks"><%= paginate @elemencharts %></div>
   <br>
<% end %>
<% @elementcharts.each do |elementchart| %>
   <p><%= link_to elementchart.name, user_elementchart_path(elementchart.user, elementchart) %></p>
   <br>
<% end %>
<p class="formlinks"><%= link_to 'Back', root_path %></p>
