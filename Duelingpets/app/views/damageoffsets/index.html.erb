<p id="notice"><%= notice %></p>

<h1>Damageoffsets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @damageoffsets.each do |damageoffset| %>
      <tr>
        <td><%= damageoffset.name %></td>
        <td><%= damageoffset.value %></td>
        <td><%= link_to 'Show', damageoffset %></td>
        <td><%= link_to 'Edit', edit_damageoffset_path(damageoffset) %></td>
        <td><%= link_to 'Destroy', damageoffset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Damageoffset', new_damageoffset_path %>
