<%= form_with(model: [@warehouse, @witemshelf], local: true) do |form| %>
  <% if @witemshelf.errors.any? %>
     <div class="alignboxes">
        <div id="errorexplan">
           <h2 id="errorheader"><%= pluralize(@witemshelf.errors.count, "error") %> prohibited this witemshelf from being saved:</h2>
           <ul>
              <% @witemshelf.errors.full_messages.each do |message| %>
                 <li class="explantext"><%= message %></li>
              <% end %>
           </ul>
        </div>
     </div>
  <% end %>
  <div class="pageformat">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <br>
  <div class="pageformat">
    <%= form.submit class: "button submitbutton" %>
  </div>
  <br>
  <p class="formlinks"><%= link_to 'Back', warehouse_path(@warehouse) %></p>
<% end %>
