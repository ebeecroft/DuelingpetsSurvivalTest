<p id="notice"><%= notice %></p>

<h1>Animalstats</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Animaltype</th>
      <th>Stat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @animalstats.each do |animalstat| %>
      <tr>
        <td><%= animalstat.value %></td>
        <td><%= animalstat.animaltype_id %></td>
        <td><%= animalstat.stat_id %></td>
        <td><%= link_to 'Show', animalstat %></td>
        <td><%= link_to 'Edit', edit_animalstat_path(animalstat) %></td>
        <td><%= link_to 'Destroy', animalstat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Animalstat', new_animalstat_path %>
