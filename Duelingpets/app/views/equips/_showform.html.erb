<div class="container">
   <div class="container"><p class="formlinks"><%= link_to getEquipItemName(slotindex, slot, "Name"), user_item_path(getEquipItemName(slotindex, slot, "Creator"), getEquipItemName(slotindex, slot, "Name")) %></p></div> <div class="container"><%= button_to 'Unequip', equipslots_unequip_path(:equipslot_id=>equipslot.id, :slotindex_id=>slotindex), method: :post, class: "button editbutton" %></div>
   <% if getEquipItemImage(slotindex, slot, "Imagecheck").to_s != "" %>
      <p><%= image_tag(getEquipItemImage(slotindex, slot, "Image")) %></p>
   <% end %>
   <p>Durability: <%= getEquipDurability(slotindex, slot, "Current") %>/<%= getEquipDurability(slotindex, slot, "Starter") %></p>
   <p>Rarity: <%= getEquipItemName(slotindex, slot, "Rarity") %></p>
   <p>Equipable: <%= getEquipItemName(slotindex, slot, "Equip") %></p>
   <p>Type: <%= getEquipItemName(slotindex, slot, "Itemtype") %></p>
   <p>Cost: <%= getEquipItemName(slotindex, slot, "Points") %></p>
   <p>Emeralds: <%= getEquipItemName(slotindex, slot, "Emeralds") %></p>
<p class="formlinks">Artist: <%= getUserPrivilege(getItemName(slotindex, slot, "User")) %><%= link_to getItemName(slotindex, slot, "Creator"), user_path(getItemName(slotindex, slot, "Creator")) %></p>
   <br>
   <p>Stats</p>
   <%= getEquipStats(slotindex, slot) %>
</div>
