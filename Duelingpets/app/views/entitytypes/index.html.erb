<p id="notice"><%= notice %></p>

<h1>Entitytypes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entitytypes.each do |entitytype| %>
      <tr>
        <td><%= entitytype.name %></td>
        <td><%= link_to 'Show', entitytype %></td>
        <td><%= link_to 'Edit', edit_entitytype_path(entitytype) %></td>
        <td><%= link_to 'Destroy', entitytype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entitytype', new_entitytype_path %>
